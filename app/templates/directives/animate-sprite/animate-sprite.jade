.animate-sprite-wrapper(style="width:{{::vm.width}}" ng-click="vm.playAnimateSprite()")
  moi-sound(sound="{{::vm.options.sound}}", ng-if="::vm.options.sound", callback="vm.endSound()")
  .img-animate(ng-style="vm.css" style="padding-bottom:{{::vm.height}}"
               tooltip-moi="{{::vm.options.name}}"
               tooltip-position="up"
               ng-mouseover="vm.increaseSize(true)"
               ng-mouseleave="vm.increaseSize(false)")
